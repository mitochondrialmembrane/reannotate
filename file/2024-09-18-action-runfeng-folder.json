        "start_frame_id": 0,
        "end_frame_id": 250
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "034",
        "annotation": "Carefully pull out the file clipped at the top from the envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": 179
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "035",
