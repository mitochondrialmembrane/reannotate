        "start_frame_id": 0,
        "end_frame_id": 103
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "017",
        "annotation": "Write \"Roses are red, Violets are blue\" on one line.\n",
        "start_frame_id": 0,
        "end_frame_id": 585
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "018",
        "annotation": "Switch to another line and prepare for writing.\n",
        "start_frame_id": 0,
        "end_frame_id": 95
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "019",
        "annotation": "Write \"That's what they say, But it just isn't true.\" on another line.\n",
        "start_frame_id": 0,
        "end_frame_id": 643
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "020",
        "annotation": "Cap the pen.\n",
        "start_frame_id": 0,
        "end_frame_id": 173
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "021",
        "annotation": "Place the pen on the table.\n",
        "start_frame_id": 0,
        "end_frame_id": 98
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
