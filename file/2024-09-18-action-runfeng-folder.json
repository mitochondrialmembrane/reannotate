[
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "000",
        "annotation": "Grip the string of the button envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": 159
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "001",
        "annotation": "Unroll the string to open the button envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": 328
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "002",
        "annotation": "Remove the files from the file folder by grasping them with your fingers and lifting them out.\n",
        "start_frame_id": 0,
        "end_frame_id": 394
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "003",
        "annotation": "Invert the file folder over a flat surface.\n",
        "start_frame_id": 0,
        "end_frame_id": 125
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "004",
        "annotation": "Shake the file folder to ensure all contents have fallen out.\n",
        "start_frame_id": 0,
        "end_frame_id": 186
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "005",
        "annotation": "Set aside the file folder.\n",
        "start_frame_id": 0,
        "end_frame_id": 148
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "006",
        "annotation": "Reorder all the contents into a pile, making sure the letter envelope is on top and the clasp envelope is on bottom\n",
        "start_frame_id": 0,
        "end_frame_id": 263
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "007",
        "annotation": "Pick up the letter opener",
        "start_frame_id": 0,
        "end_frame_id": 69
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "007",
        "annotation": "Slit the top edge of the envelope with a letter opener, ensuring not to damage the contents inside.\n",
        "start_frame_id": 70,
        "end_frame_id": 252
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "008",
        "annotation": "Extract the bound letter carefully to avoid tearing any pages.\n",
        "start_frame_id": 0,
        "end_frame_id": 190
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "009",
        "annotation": "Unfold the letter by reversing the folds with gentle hand movements.\n",
        "start_frame_id": 0,
        "end_frame_id": 185
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "010",
        "annotation": "Press down on the unfolded letter with both hands to flatten and remove any creases.\n",
        "start_frame_id": 0,
        "end_frame_id": 183
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "011",
        "annotation": "Lay the unfolded letter on a flat surface for reading.\n",
        "start_frame_id": 0,
        "end_frame_id": 133
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "012",
        "annotation": "Adjust the angles to straighten the paper for easy reading.\n",
        "start_frame_id": 0,
        "end_frame_id": 168
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "013",
        "annotation": "Read the letter by moving your eyes while your hands hold the edges to keep the paper steady.\n",
        "start_frame_id": 0,
        "end_frame_id": 239
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "014",
        "annotation": "Remove the staple bind with the staple remover.\n",
        "start_frame_id": 0,
        "end_frame_id": 639
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "015",
        "annotation": "Grasp the pen.\n",
        "start_frame_id": 0,
        "end_frame_id": 101
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
