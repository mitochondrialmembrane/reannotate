        "annotation": "Realign letter papers to get ready for binding.\n",
        "start_frame_id": 0,
        "end_frame_id": 236
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "023",
        "annotation": "Grab the stapler",
        "start_frame_id": 0,
        "end_frame_id": 79
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "023",
        "annotation": "Staple the letter to secure documents together with the stapler.\n",
        "start_frame_id": 80,
        "end_frame_id": 179
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "023",
        "annotation": "Put down the stapler",
        "start_frame_id": 180,
        "end_frame_id": 263
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "024",
        "annotation": "Fold the letter into thirds, making precise folds to ensure a compact shape.\n",
        "start_frame_id": 0,
        "end_frame_id": 482
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "025",
        "annotation": "Put the letter into the envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": 355
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "026",
        "annotation": "Grab the glue stick.\n",
        "start_frame_id": 0,
        "end_frame_id": 92
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "027",
        "annotation": "Uncap the glue stick.\n",
        "start_frame_id": 0,
        "end_frame_id": 197
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "028",
        "annotation": "Twist the end of the glue stick",
        "start_frame_id": 0,
        "end_frame_id": 115
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "028",
        "annotation": "Glue along the edge of the envelope.\n",
        "start_frame_id": 116,
        "end_frame_id": 317
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "029",
        "annotation": "Press the envelope to seal.\n",
        "start_frame_id": 0,
        "end_frame_id": 176
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "030",
        "annotation": "Twist the gluestick end to retract the glue",
        "start_frame_id": 0,
        "end_frame_id": 194
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "030",
        "annotation": "Cap the glue stick.\n",
        "start_frame_id": 195,
        "end_frame_id": 316
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
        "sequence": "031",
        "annotation": "Put down the glue stick.\n",
        "start_frame_id": 0,
        "end_frame_id": 99
    },
    {
        "scene": "2024-09-18-action-runfeng-folder",
