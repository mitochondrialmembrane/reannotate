        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-09-18-action-arman-monopoly",
        "sequence": "063",
        "annotation": "Play a round of knucklebones with the coin.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-09-18-action-arman-monopoly",
        "sequence": "064",
