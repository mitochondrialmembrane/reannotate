        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-09-18-action-arman-monopoly",
        "sequence": "045",
        "annotation": "Alter back the location of two piles.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-09-18-action-arman-monopoly",
        "sequence": "046",
