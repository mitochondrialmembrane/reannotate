[
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "000",
        "annotation": "Uncap the hand sanitizer bottle.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "001",
        "annotation": "Apply a generous amount of sanitizer to your dominant hand.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "002",
        "annotation": "Cap the hand sanitizer bottle.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "003",
        "annotation": "Put the sanitizer bottle down on the table.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "004",
        "annotation": "Rub your hands together to sanitize them thoroughly.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "005",
        "annotation": "Hold the can of soda.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "006",
        "annotation": "Pull the ring tab to open the soda can.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "007",
        "annotation": "Insert a straw into the soda can.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "008",
        "annotation": "Drink from the soda with the straw.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "009",
        "annotation": "Put the soda can down.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "010",
        "annotation": "Unwrap the burger and set the paper wrapper aside.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "011",
        "annotation": "Deliver the burger to your mouth and take one or several bites.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "012",
        "annotation": "Put the burger back on the plate.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "013",
        "annotation": "Squeeze some ketchup out from the ketchup bottle onto the plate.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "014",
        "annotation": "Grip a fry with your fingers.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "015",
        "annotation": "Dip the fry into the ketchup.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "016",
        "annotation": "Deliver the fry to your mouth and eat it.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "017",
        "annotation": "Sprinkle any remaining salt on your fingers over the plate.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "018",
        "annotation": "Peel off the lid of the dipping sauce container.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "019",
        "annotation": "Grasp a piece of fried chicken thigh.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "020",
        "annotation": "Dip the fried chicken thigh into the dipping sauce.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "021",
        "annotation": "Rotate the chicken thigh as you eat it to get all sides.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "022",
        "annotation": "Pick up one piece of napkin.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "023",
        "annotation": "Unfold the napkin.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "024",
        "annotation": "Wipe any crumbs or spills off the table with a napkin.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "025",
        "annotation": "Scrunch the napkin into a ball.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "026",
        "annotation": "Drop the napkin ball into the trash can.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "027",
        "annotation": "Grasp the jar containing the pudding.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "028",
        "annotation": "Pinch the rubber band around the pudding jar.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "029",
        "annotation": "Stretch the rubber band and slide it off the jar.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "030",
        "annotation": "Remove the cloth covering the jar to reveal the pudding.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "031",
        "annotation": "Hold the pudding jar over a plate.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "032",
        "annotation": "Invert the pudding jar to dump the pudding onto the plate.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "033",
        "annotation": "Shake the jar to ensure all the pudding is transferred to the plate.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "034",
        "annotation": "Scoop with a spoon to ensure all the puddings are transferred.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "035",
        "annotation": "Put down the pudding jar.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "036",
        "annotation": "Grip the pudding spoon.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "037",
        "annotation": "Scoop a piece of pudding.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "038",
        "annotation": "Deliver the pudding to your mouth and eat it.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "039",
        "annotation": "Put down the pudding spoon.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "040",
        "annotation": "Cover the remaining pudding with the pudding jar.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "041",
        "annotation": "Flip the pudding jar and plate to transfer the remaining pudding back into the jar.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "042",
        "annotation": "Lift the piece of cloth off the table.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "043",
        "annotation": "Cover the pudding jar with the cloth.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "044",
        "annotation": "Grip the rubber band.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "045",
        "annotation": "Bind the pudding jar with the rubber band.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "046",
        "annotation": "Put the pudding jar back on the table.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "047",
        "annotation": "Gather the remaining chicken thigh and fries back into the food box.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "048",
        "annotation": "Wrap the remaining burger with the paper wrapper.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "049",
        "annotation": "Close the food box.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "050",
        "annotation": "Pull out the straw from the soda can.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "051",
        "annotation": "Wedge the used straw into the soda can opening.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "052",
        "annotation": "Gather the remaining food items and place them back into the paper bag.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "053",
        "annotation": "Fold the top of the paper bag to close it.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-08-30-action-nick-fastfood",
        "sequence": "054",
        "annotation": "Clean your hands with hand sanitizer.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    }
]