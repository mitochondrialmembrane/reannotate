        "end_frame_id": -1
    },
    {
        "scene": "2024-11-01-action-maxim-stone",
        "sequence": "005",
        "annotation": "Park your stone carving tools neatly beside the work area.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-11-01-action-maxim-stone",
