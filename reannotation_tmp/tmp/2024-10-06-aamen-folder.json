[
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "000",
        "annotation": "Grip the string of the button envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "001",
        "annotation": "Unroll the string to open the button envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "002",
        "annotation": "Remove the files from the file folder by grasping them with your fingers and lifting them out.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "003",
        "annotation": "Invert the file folder over a flat surface.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "004",
        "annotation": "Set aside the file folder.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "005",
        "annotation": "Reorder all the contents into a pile, making sure the letter envelope is on top and the clasp envelope is on bottom\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "006",
        "annotation": "Slit the top edge of the envelope with a letter opener, ensuring not to damage the contents inside.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "007",
        "annotation": "Extract the bound letter carefully to avoid tearing any pages.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "008",
        "annotation": "Unfold the letter by reversing the folds with gentle hand movements.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "009",
        "annotation": "Press down on the unfolded letter with both hands to flatten and remove any creases.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "010",
        "annotation": "Adjust the angles to straighten the paper for easy reading.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "011",
        "annotation": "Remove the staple bind with the staple remover.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "012",
        "annotation": "Grasp the pen.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "013",
        "annotation": "Uncap the pen.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "014",
        "annotation": "Write \"Roses are red, Violets are blue\" on one line.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "015",
        "annotation": "Switch to another line and prepare for writing.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "016",
        "annotation": "Write \"That's what they say, But it just isn't true.\" on another line.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "017",
        "annotation": "Cap the pen.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "018",
        "annotation": "Place the pen on the table.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "019",
        "annotation": "Realign letter papers to get ready for binding.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "020",
        "annotation": "Staple the letter to secure documents together with the stapler.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "021",
        "annotation": "Fold the letter into thirds, making precise folds to ensure a compact shape.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "022",
        "annotation": "Put the letter into the envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "023",
        "annotation": "Grab the glue stick.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "024",
        "annotation": "Uncap the glue stick.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "025",
        "annotation": "Glue along the edge of the envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "026",
        "annotation": "Press the envelope to seal.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "027",
        "annotation": "Cap the glue stick.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "028",
        "annotation": "Put down the glue stick.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "029",
        "annotation": "Stamp the corner of the envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "030",
        "annotation": "Unclasp the metal clasp of the envelope by lifting and pulling it away from the slot.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "031",
        "annotation": "Carefully pull out the file clipped at the top from the envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "032",
        "annotation": "Reverse the folds of the clipped document.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "033",
        "annotation": "Read the document attentively, holding it with one hand and using the other to help navigate pages.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "034",
        "annotation": "Unclip the clip from the top of the file by sliding it off.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "035",
        "annotation": "Take out one page of the paper.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "036",
        "annotation": "Crumple the taken-out page into a ball and set it aside.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "037",
        "annotation": "Attach a paper clip to the top of the remaining aligned files to keep them together.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "038",
        "annotation": "Fold the stacked files along the predetermined lines for compact storage.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "039",
        "annotation": "Insert the folded files into a clasp envelope, aligning them within.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "040",
        "annotation": "Clasp the metal clasp of the envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "041",
        "annotation": "Flip the file to the third page.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "042",
        "annotation": "Sign the third page using a smooth, flowing motion.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "043",
        "annotation": "Open the side clamp by pressing the lever and gently pulling it away from the file.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "044",
        "annotation": "Switch the signed page to the front page.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "045",
        "annotation": "Arrange the signed files side by side, aligning them by their edges.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "046",
        "annotation": "Secure the signed document within a clamp holder by pressing down on the clamp and sliding it into place.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "047",
        "annotation": "Pile the clamped file, the clasp envelope and the letter envelope into a stack, with the letter envelope on bottom and the clasp envelope on top.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "048",
        "annotation": "Insert the stacked content into the button envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    },
    {
        "scene": "2024-10-06-aamen-folder",
        "sequence": "049",
        "annotation": "Roll the string to close the button envelope.\n",
        "start_frame_id": 0,
        "end_frame_id": -1
    }
]