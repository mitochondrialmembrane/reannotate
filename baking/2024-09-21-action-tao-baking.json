        "start_frame_id": 0,
        "end_frame_id": 168
    },
    {
        "scene": "2024-09-21-action-tao-baking",
        "sequence": "039",
        "annotation": "Shut the oven door.\n",
        "start_frame_id": 0,
        "end_frame_id": 121
    },
    {
        "scene": "2024-09-21-action-tao-baking",
        "sequence": "040",
        "annotation": "Open the oven door.\n",
        "start_frame_id": 0,
        "end_frame_id": 112
    },
    {
        "scene": "2024-09-21-action-tao-baking",
        "sequence": "041",
