        "start_frame_id": 0,
        "end_frame_id": 211
    },
    {
        "scene": "2024-09-11-action-austin-baking2",
        "sequence": "026",
        "annotation": "Shut the oven door.\n",
        "start_frame_id": 0,
        "end_frame_id": 137
    },
    {
        "scene": "2024-09-11-action-austin-baking2",
