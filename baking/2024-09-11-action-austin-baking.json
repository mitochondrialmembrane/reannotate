        "start_frame_id": 0,
        "end_frame_id": 302
    },
    {
        "scene": "2024-09-11-action-austin-baking",
        "sequence": "009",
        "annotation": "Grab the yeast bag",
        "start_frame_id": 0,
        "end_frame_id": 124
    },
    {
        "scene": "2024-09-11-action-austin-baking",
        "sequence": "009",
        "annotation": "Open the yeast bag",
        "start_frame_id": 125,
        "end_frame_id": 219
    },
    {
        "scene": "2024-09-11-action-austin-baking",
        "sequence": "009",
        "annotation": "Scoop a one quater of fourth spoonful of yeast.\n",
        "start_frame_id": 220,
        "end_frame_id": 656
    },
    {
        "scene": "2024-09-11-action-austin-baking",
        "sequence": "010",
        "annotation": "Add the yeast to the mixing bowl.\n",
        "start_frame_id": 0,
        "end_frame_id": 144
    },
    {
        "scene": "2024-09-11-action-austin-baking",
        "sequence": "011",
        "annotation": "Stir the ingredients with a spatula.\n",
        "start_frame_id": 0,
        "end_frame_id": 602
    },
    {
        "scene": "2024-09-11-action-austin-baking",
