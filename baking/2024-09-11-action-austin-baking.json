        "start_frame_id": 0,
        "end_frame_id": 231
    },
    {
        "scene": "2024-09-11-action-austin-baking2",
        "sequence": "023",
        "annotation": "Shut the oven door.\n",
        "start_frame_id": 0,
        "end_frame_id": 133
    },
    {
        "scene": "2024-09-11-action-austin-baking2",
