        "scene": "2024-08-12-action-chaerin-baking",
        "sequence": "001",
        "annotation": "Move the bowl",
        "start_frame_id": 140,
        "end_frame_id": 189
    },
    {
        "scene": "2024-08-12-action-chaerin-baking",
        "sequence": "001",
